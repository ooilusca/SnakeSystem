  inherited FDUCadPadrao: TFDUpdateSQL
    InsertSQL.Strings = (
      'INSERT INTO CLIENTE'
      '(ID, ID_CIDADE, NOME, CPF, CELULAR, '
      '  EMAIL, ENDERECO, NUMERO, BAIRRO, CEP)'
      
        'VALUES (:NEW_ID, :NEW_ID_CIDADE, :NEW_NOME, :NEW_CPF, :NEW_CELUL' +
        'AR, '
      '  :NEW_EMAIL, :NEW_ENDERECO, :NEW_NUMERO, :NEW_BAIRRO, :NEW_CEP)'
      
        'RETURNING ID, ID_CIDADE, NOME, CPF, CELULAR, EMAIL, ENDERECO, NU' +
        'MERO, BAIRRO, CEP')
    ModifySQL.Strings = (
      'UPDATE CLIENTE'
      'SET ID = :NEW_ID, ID_CIDADE = :NEW_ID_CIDADE, NOME = :NEW_NOME, '
      '  CPF = :NEW_CPF, CELULAR = :NEW_CELULAR, EMAIL = :NEW_EMAIL, '
      
        '  ENDERECO = :NEW_ENDERECO, NUMERO = :NEW_NUMERO, BAIRRO = :NEW_' +
        'BAIRRO, '
      '  CEP = :NEW_CEP'
      'WHERE ID = :OLD_ID'
      
        'RETURNING ID, ID_CIDADE, NOME, CPF, CELULAR, EMAIL, ENDERECO, NU' +
        'MERO, BAIRRO, CEP')
    DeleteSQL.Strings = (
      'DELETE FROM CLIENTE'
      'WHERE ID = :OLD_ID')
    FetchRowSQL.Strings = (
      
        'SELECT ID, ID_CIDADE, NOME, CPF, CELULAR, EMAIL, ENDERECO, NUMER' +
        'O, '
      '  BAIRRO, CEP'
      'FROM CLIENTE'
      'WHERE ID = :OLD_ID')
    Left = 443
    Top = 8
  end
