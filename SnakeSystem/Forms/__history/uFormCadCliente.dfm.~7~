    UpdateOptions.FetchGeneratorsPoint = gpImmediate
    UpdateOptions.GeneratorName = 'GEN_CLIENTE_ID'
    UpdateOptions.AutoIncFields = 'ID_CLIENTE'
    UpdateObject = nil
    SQL.Strings = (
      'SELECT CLI.ID ID_CLIENTE, CLI.ID_CIDADE, CLI.NOME CLIENTE,'
      '       CLI.CPF, CLI.CELULAR, CLI.EMAIL,'
      
        '       CLI.ENDERECO, CLI.NUMERO, CLI.BAIRRO, CLI.CEP, CLI.DATA_E' +
        'XCLUIDO'
      'FROM CLIENTE CLI')
